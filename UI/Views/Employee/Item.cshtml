@model Objects.Employee
@{
    Layout = null;
}
<form id="form">
    @Html.HiddenFor(m => Model.QuoteId)

    <div class="form-group">
        <label>
            First Name
        </label>
        <div>
            @Html.TextBoxFor(m => Model.FirstName, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        <label>
            Last Name
        </label>
        <div>
            @Html.TextBoxFor(m => Model.LastName, new { @class = "form-control" })
        </div>
    </div>
</form>